{
    "name": "proompting",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"wrangler dev\" \"tailwindcss -i ./public/input.css -o ./public/output.css --watch\"",
        "deploy": "tailwindcss -i ./public/input.css -o ./public/output.css && wrangler deploy --minify",
        "cf-typegen": "wrangler types --env-interface CloudflareBindings"
    },
    "dependencies": {
        "@google/genai": "^1.17.0",
        "hono": "^4.9.6"
    },
    "devDependencies": {
        "@biomejs/biome": "2.2.3",
        "concurrently": "^9.2.1",
        "wrangler": "^4.34.0",
        "tailwindcss": "^4.1.13"
    }
}
